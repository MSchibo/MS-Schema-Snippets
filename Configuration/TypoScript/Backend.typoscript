# Backend-Modul-Views für das Extbase-Modul "LegacyModule" in TYPO3 11

module.tx_siterichsnippets {
  view {
    # Hier liegen deine Queue/Show-Templates
    templateRootPaths {
      0 = EXT:site_richsnippets/Resources/Private/Templates/
    }

    # Optional – kannst du später nutzen
    partialRootPaths {
      0 = EXT:site_richsnippets/Resources/Private/Partials/
    }
    layoutRootPaths {
      0 = EXT:site_richsnippets/Resources/Private/Layouts/
    }
  }
}
